{
  "name": "@inove-ai/inove-ai-framework",
  "version": "1.0.1",
  "description": "Inove AI Framework - Multi-Agent AI Development Kit",
  "bin": {
    "inove-ai-framework": "./bin/cli.js"
  },
  "files": [
    ".agents/**",
    "!.agents/.shared/**",
    "!.agents/locks/**",
    "!**/__pycache__/**",
    "!**/*.pyc",
    "bin",
    "CLAUDE.md",
    "AGENTS.md",
    "GEMINI.md"
  ],
  "scripts": {
    "dev": "pnpm --prefix app dev",
    "dev:emulators": "concurrently \"firebase emulators:start\" \"pnpm --prefix app dev\"",
    "emulators": "firebase emulators:start",
    "seed": "npx tsx scripts/seed-emulator.ts",
    "build": "pnpm --prefix app build",
    "start": "pnpm --prefix app preview",
    "lint": "pnpm --prefix app lint",
    "typecheck": "pnpm --prefix app typecheck",
    "test": "python3 -m pytest tests/ -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoelBonito/inove-ai-framework.git"
  },
  "keywords": [
    "ai",
    "agent",
    "framework",
    "inove",
    "claude",
    "gemini"
  ],
  "author": "JoelBonito",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "tsx": "^4.21.0"
  },
  "dependencies": {
    "firebase": "^12.9.0",
    "firebase-functions": "^7.0.5"
  }
}
