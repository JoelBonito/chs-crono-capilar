{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd app && pnpm install && pnpm build",
  "outputDirectory": "app/dist",
  "installCommand": "echo 'Using custom build command'",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
